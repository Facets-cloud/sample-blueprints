{
  "$schema": "https://facets-cloud.github.io/facets-schemas/schemas/service/service.schema.json",
  "flavor": "default",
  "metadata": {
    "labels": {},
    "git": {
      "repository_url": "https://github.com/Facets-cloud/sample-blueprints.git",
      "path": "spring-boot-reactjs-ts-mysql-template /blueprint"
    }
  },
  "kind": "service",
  "disabled": false,
  "provided": false,
  "version": "0.1",
  "spec": {
    "enable_host_anti_affinity": false,
    "type": "application",
    "release": {
      "strategy": {
        "type": "RollingUpdate"
      },
      "build": {
        "artifactory": "facets-config-auto",
        "name": "reactjs-petclinic-CI"
      }
    },
    "runtime": {
      "autoscaling": {
        "min": "1",
        "max": "1",
        "cpu_threshold": "50"
      },
      "ports": {
        "http": {
          "port": "8080",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "300m",
        "memory": "1Gi",
        "cpu_limit": "1000m"
      },
      "health_checks": {
        "timeout": "10",
        "period": "30",
        "port": "8080",
        "start_up_time": "15"
      },
      "volumes": {}
    },
    "env": {
      "LOG_LEVEL": "INFO"
    }
  }
}